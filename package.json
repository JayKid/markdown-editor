{
  "name": "tuto",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "marked": "^0.3.6",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.5",
    "serve": "^5.1.3",
    "sw-precache": "^4.2.2"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build && sw-precache --config=sw-precache-config.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "serve": "node_modules/serve/bin/serve.js -s build",
    "build-serve": "react-scripts build && sw-precache --config=sw-precache-config.js && node_modules/serve/bin/serve.js -s build"
  }
}
